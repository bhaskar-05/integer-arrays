#include <iostream>
using namespace std;
void print_Arr( int arr[] , int size) {
    
    for (int i=0; i<size ; i++) {
        
        cout << arr[i] << " ";
    }
}
  
void rev_Arr ( int arr[] , int n) {
    
    int start= 0;
    int end = n-1;
    while ( start <= end) {
        
        swap( arr[start], arr[end]);
        start++;
        end--;
    }
    
    
}

int main() {
    
    int arr[]={2,6,5,7,8,9,4,5};
    int brr[]= {8,6,5,7,1,69,6,3,4};
    
    rev_Arr(arr, 8);
    print_Arr(arr, 8);
    rev_Arr(brr, 9);
    print_Arr(brr, 9);
    
    
}
