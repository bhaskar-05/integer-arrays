#include <iostream>
using namespace std;

int main() {
    
    int arr1[]= {1,5,8,4,92,4};
    int arr2[]= {1,5,4,8,6,5,44};
    
    for( int i=0; i<6; i++) {
        
        int j=0;
        while (j<7) {
            
            if (arr1[i] == arr2[j]) {
                
                cout << arr1[i]<< " ";
                arr2[j]=-2; //update kiya taaki ye value ab baaki aur kisi k saath match na hojaaye
                arr1[i]=-4; //yahan bhi same reason
                
            }
            j++;
            
        }
    }
}
